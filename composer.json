{
	"name": "bitsnbolts/laravel-analytics",
	"type": "library",
	"description": "Analytics tracking for Laravel",
	"keywords": ["analytics", "google analytics", "laravel", "statistics", "javascript", "php"],
	"license": "MIT",
	"authors": [
		{
			"name": "Thijs van den Anker",
			"email": "thijs@bitsnbolts.nl"
		}
	],
	"require": {
		"php": "^7.2 || ^8.0 || ^8.1",
		"illuminate/config": "~5.0 || ^6.20 || ^7.24 || ^8.40 || ^9.31",
		"illuminate/session": "~5.0 || ^6.20 || ^7.24 || ^8.40 || ^9.31",
		"illuminate/support": "~5.0 || ^6.20 || ^7.24 || ^8.40 || ^9.31"
	},
	"require-dev": {
		"laravel/framework": "~5.0 || ^6.20 || ^7.24 || ^8.40 || ^9.31"
	},
	"autoload": {
		"psr-0": {
			"Bitsnbolts\\LaravelAnalytics\\": "src/"
		}
	},
	"minimum-stability": "stable",
	"suggest": {
		"martijnc/php-csp": "For handling Content Security Policy matters"
	},
	"support": {
		"issues": "https://github.com/bitsnbolts/laravel-analytics/issues",
		"source": "https://github.com/bitsnbolts/laravel-analytics/archive/master.zip"
	},
	"extra": {
		"laravel": {
			"providers": [
				"Bitsnbolts\\LaravelAnalytics\\AnalyticsServiceProvider"
			],
			"aliases": {
				"Analytics": "Bitsnbolts\\LaravelAnalytics\\AnalyticsFacade"
			}
		}
	}
}
